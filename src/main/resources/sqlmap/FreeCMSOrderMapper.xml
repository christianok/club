<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.earth.message.mapper.FreeCMSOrderMapper">
    <resultMap id="BaseResultMap" type="com.earth.message.model.dao.FreeCMSOrder">
        <id column="id" jdbcType="VARCHAR" property="id"/>
        <result column="dataid" jdbcType="VARCHAR" property="dataId"/>
        <result column="consignee" jdbcType="VARCHAR" property="consignee"/>
        <result column="order_annex" jdbcType="VARCHAR" property="orderAnnex"/>
        <result column="order_check" jdbcType="VARCHAR" property="orderCheck"/>
    </resultMap>

    <select id="selectById" resultMap="BaseResultMap">
      select * from freecms_order where id >= #{id}
    </select>

</mapper>